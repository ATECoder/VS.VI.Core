<?xml version="1.0" encoding="utf-8"?>
<configuration>

  <configSections>
    <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup" >
      <section name="isr.VI.Tsp2.K2450.Tests.K2450TestInfo" type="System.Configuration.ClientSettingsSection" />
      <section name="isr.VI.Tsp2.K2450.Tests.K2450ResistanceTestInfo" type="System.Configuration.ClientSettingsSection" />
    </sectionGroup>
  </configSections>

  <appSettings>
    <add key="Exists" value="True" />
    <add key="Verbose" value="True" />
    <add key="All" value="True" />
    <add key="LocalTimeZone" value="Pacific Standard Time" />
    <add key="LocalTimeZoneOffset" value="-8" />
    <add key="RemoteTimeZone" value="Central Standard Time" />
    <add key="RemoteTimeZoneOffset" value="-6" />
    <add key="LocalIPv4" value ="192.168"/>
    <add key="RemoteIPv4" value ="20.1"/>
  </appSettings>

  <userSettings>
    
     <!-- DEVICE TEST -->
    <isr.VI.Tsp2.K2450.Tests.K2450TestInfo>
      
      <!-- CONFIGURATION -->
      <setting name="Exists" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="Verbose" serializeAs="String">
        <value>True</value>
      </setting>

      <!-- RESOURCE -->
      <setting name="ResourceModel"  serializeAs="String">
        <value>2450</value>
      </setting>
      <setting name="FirstResourceName"  serializeAs="String">
        <value>TCPIP0::192.168.0.152::inst0::INSTR</value>
      </setting>
      <setting name="SecondResourceName"  serializeAs="String">
        <value>TCPIP0::192.168.0.156::inst0::INSTR</value>
      </setting>
      <setting name="ThirdResourceName"  serializeAs="String">
        <value>TCPIP0::20.1.1.21::inst0::INSTR</value>
      </setting>
      <setting name="FourthResourceName"  serializeAs="String">
        <value>TCPIP0::20.1.1.24::inst0::INSTR</value>
      </setting>
      <setting name="ResourceTitle"  serializeAs="String">
        <value>2450</value>
      </setting>

      <!-- VISA SESSION -->
      <setting name="KeepAliveQueryCommand" serializeAs="String">
        <value>*OPC?</value>
      </setting>
      <setting name="KeepAliveCommand"  serializeAs="String">
        <value>*OPC</value>
      </setting>
      <setting name="ReadTerminationEnabled" serializeAs="String">
        <value>False</value>
      </setting>
      <setting name="TerminationCharacter" serializeAs="String">
        <value>10</value>
      </setting>

      <!-- INITIAL MEASURE VALUES -->
      <setting name="InitialPowerLineCycles" serializeAs="String">
        <value>1</value>
      </setting>
      <setting name="InitialAutoZeroEnabled" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="InitialAutoRangeEnabled" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="InitialMeasureFunction" serializeAs="String">
        <value>2</value>
      </setting>
      <setting name="InitialRemoteSenseSelected" serializeAs="String">
        <value>False</value>
      </setting>

      <!-- SMU SETTINGS -->
      <setting name="LineFrequency"  serializeAs="String">
        <value>60</value>
      </setting>
      <setting name="MaximumOutputPower" serializeAs="String">
        <value>22</value>
      </setting>

      <!-- INITIAL SOURCE VALUES -->
      <setting name="InitialFrontTerminalsSelected" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="InitialSourceFunction" serializeAs="String">
        <value>1</value>
      </setting>
      <setting name="InitialAutoDelayEnabled" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="InitialSourceLevel" serializeAs="String">
        <value>0</value>
      </setting>
      <setting name="InitialSourceLimit" serializeAs="String">
        <value>0.000105</value>
      </setting>
    </isr.VI.Tsp2.K2450.Tests.K2450TestInfo>

    <!-- RESISTANCE -->
    <isr.VI.Tsp2.K2450.Tests.K2450ResistanceTestInfo>

      <!-- CONFIGURATION -->
      <setting name="Exists" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="Verbose" serializeAs="String">
        <value>True</value>
      </setting>

      <!-- MEASUREMENT SETTINGS -->
      <setting name="AutoZeroEnabled" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="AutoRangeEnabled" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="SenseFunction" serializeAs="String">
        <value>3</value>
      </setting>
      <setting name="PowerLineCycles" serializeAs="String">
        <value>1</value>
      </setting>

      <!-- RESISTANCE MEASUREMENT: SHORT-->
      <setting name="ExpectedResistance" serializeAs="String">
        <value>0</value>
      </setting>
      <setting name="ExpectedResistanceEpsilon" serializeAs="String">
        <value>0.01</value>
      </setting>

      <setting name="FrontTerminalsSelected" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="RemoteSenseSelected" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="SourceFunction" serializeAs="String">
        <value>2</value>
      </setting>
      <setting name="SourceLevel" serializeAs="String">
        <value>0.02</value>
      </setting>

    </isr.VI.Tsp2.K2450.Tests.K2450ResistanceTestInfo>
  </userSettings>

</configuration>
